                        </ul>
                    </nav>
                </div>
                <!-- nav -->
            </div>
        </div>
    </header>
    <!-- header -->
    @RenderBody()
    <!-- footer -->
    <footer role="footer">
        <!-- nav -->
        <nav role="footer-nav">
            <ul>
                <li id="footer_nav_iSelect"><a href="index.html" title="iSelect">iSelect</a></li>
                <li id="footer_nav_3CNews"><a href="about.html" title="大話3C">大話3C</a></li>
